{
  "name": "jsaqa_3",
  "version": "1.0.0",
  "description": "jest test",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --collectCoverage"
  },
  "keywords": [
    "jest",
    "test",
    "jsaqa"
  ],
  "author": "vavlauz",
  "license": "ISC",
  "devDependencies": {
    "jest": "^27.5.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/.vscode/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  }
}
